{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/index.ts", 
      "use": "@vercel/node" 
    }
  ],
  "routes": [
    {
      "src": "/api/scores",
      "dest": "backend/src/index.ts" 
    },
    {
      "src": "/(.*)",
      "dest": "backend/src/index.ts" 
    }
  ]
}